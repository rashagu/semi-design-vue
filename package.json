{
  "name": "semi-ui-vue",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -C ./packages/semi-ui-vue dev",
    "test:unit": "vitest --environment jsdom",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "build:lib": "pnpm -C ./packages/semi-animation-vue build:lib & pnpm -C ./packages/semi-icons-vue build:lib & pnpm -C ./packages/semi-illustrations-vue build:lib & pnpm -C ./packages/semi-ui-vue build:vite",
    "pre": "build:lib",
    "build:publish": "pnpm build:lib & pnpm publish -r",
    "change": "pnpm changeset",
    "version": "pnpm changeset version",
    "publish": "pnpm changeset publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook:vite": "storybook build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "workspaces": [
    "packages/semi-animation-vue",
    "packages/semi-icons-vue",
    "packages/semi-ui-vue",
    "packages/semi-illustrations-vue"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  },
  "dependencies": {
    "@douyinfe/semi-foundation": "2.42.4",
    "@douyinfe/semi-theme-default": "2.42.4",
    "@vue/repl": "^2.6.1",
    "vue": "^3.3.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@kousum/semi-icons-vue": "^0.5.4",
    "@kousum/semi-ui-vue": "^0.5.4",
    "vue-inline-svg": "^2.1.3",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/blocks": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.1.0",
    "@storybook/vue3-vite": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.0",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.16.7",
    "@changesets/cli": "^2.26.0",
    "@douyinfe/semi-theme-default": "2.42.4",
    "@svgr/babel-plugin-add-jsx-attribute": "5.4.0",
    "@svgr/babel-plugin-remove-jsx-attribute": "5.4.0",
    "@svgr/babel-plugin-remove-jsx-empty-expression": "5.0.1",
    "@svgr/babel-plugin-replace-jsx-attribute-value": "5.0.1",
    "@svgr/babel-plugin-svg-dynamic-title": "5.4.0",
    "@svgr/babel-plugin-svg-em-dimensions": "5.4.0",
    "@svgr/babel-plugin-transform-react-native-svg": "5.4.0",
    "@svgr/babel-plugin-transform-svg-component": "5.5.0",
    "@svgr/babel-preset": "5.5.0",
    "@svgr/core": "5.5.0",
    "@svgr/hast-util-to-babel-ast": "^6.2.1",
    "@svgr/plugin-jsx": "5.5.0",
    "@svgr/plugin-svgo": "5.5.0",
    "@svgr/webpack": "5.5.0",
    "@types/node": "^20.1.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/coverage-v8": "^1.0.0-beta.5",
    "@vue/test-utils": "^2.4.2",
    "@testing-library/vue": "^6.6.1",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.1",
    "postcss-loader": "^7.0.0",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "vue-loader": "^16.8.3",
    "camelcase": "^6.3.0",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.3",
    "svgo": "2.7.0",
    "ts-node": "^10.5.0",
    "tslib": "^2.4.1",
    "vitest": "^1.0.0-beta.5",
    "typescript": "^5.1.6",
    "vite": "4.4.11",
    "vitepress": "1.0.0-rc.31",
    "vue-tsc": "^1.8.15"
  }
}
