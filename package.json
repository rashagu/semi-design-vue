    "@types/node": "^20.1.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/web-worker": "^2.1.9",
    "@vue/test-utils": "^2.4.6",
    "babel-loader": "^8.3.0",
    "camelcase": "^6.3.0",
