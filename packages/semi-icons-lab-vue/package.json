{
  "name": "@kousum/semi-icons-lab-vue",
  "version": "2.77.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vue-tsc --noEmit && rimraf lib && tsc -b --force tsconfig.esm.json && node scripts/compileLib.cjs",
    "build:vite": "rimraf lib && rimraf dist && vue-tsc --noEmit && tsc -b --force tsconfig.esm.json && node build.cjs && node scripts/compileLib.cjs && rimraf lib && vite build",
    "build:icon": "node scripts/build-icon.cjs ",
    "preview": "vite preview"
  },
  "dependencies": {
    "@douyinfe/semi-foundation": "2.77.1",
    "@douyinfe/semi-theme-default": "2.77.1",
    "classnames": "^2.3.2",
    "sass": "^1.57.1",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "vue": ">=3.4.3"
  },
  "sideEffects": [
    "*.scss",
    "*.css",
    "lib/index.js"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "camelcase": "^6.3.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "identity-obj-proxy": "^3.0.0",
    "merge2": "^1.4.1",
    "rimraf": "^3.0.2",
    "fast-glob": "^3.2.12",
    "through2": "^4.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^5.5.2",
    "vite": "6.2.5",
    "vue-tsc": "^2.1.6"
  }
}
